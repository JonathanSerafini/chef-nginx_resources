<% @configs.keys.each do |param_name| %>
  <% if param_name == 'log_formats' %>
    <% @configs['log_formats'].each do |format_name,format| %>
<%= nginx_param(format_name, format, { 'prefix' => 'log_format' }) %>
    <% end %>
  <% else %>
<%= nginx_param(param_name, @configs[param_name]) %>
  <% end %>
<% end %>
